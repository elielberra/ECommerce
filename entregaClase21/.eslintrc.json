{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": ["warn", { "varsIgnorePattern": ["^unused", "handleProductAddedToCart"] }]
  },
  "globals": {
    "io": "readonly",
    "Swal": "readonly"
  }
}
